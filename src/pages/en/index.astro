<Layout title="Radio Musicak" lang="en" dir="ltr">
  <section class="grid grid-cols-2 md:grid-cols-4 lg:grid-cols-6 w-full gap-2">
    <!-- <article class="col-span-full font-lalezar">خوش آمدید</article> -->
    {
      data.map((d: any) => {
        return (
          <div class="flex flex-col p-2 gap-1.5 border-2 rounded border-slate-300 dark:border-slate-600">
            <img
              class="aspect-square size-full object-cover rounded"
              id={d.picId}
              src={"pic/" + d.picId}
              alt={d.titleE}
            />
            <h1 class="text-base font-bold">{d.titleE}</h1>
            <h2 class="text-xs">{d.artistE}</h2>
            <p class="text-xs">{d.genreE}</p>
            <a
              class=" flex justify-between rounded bg-blue-500 hover:bg-blue-600 active:bg-blue-700 transition-all duration-300 text-gray-100 px-2 py-2 text-sm"
              href={d.file}
              target="_blank"
            >
              Download
              <span class="text-sm">{d.size}</span>
            </a>
          </div>
        );
      })
    }
  </section>
</Layout>
